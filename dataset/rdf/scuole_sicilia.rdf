  <rdf:Description rdf:about="http://www.semanticweb.org/andre/scuole-sicilia/resource/RGPS01000R-valutazione22">
    <rdf:type rdf:resource="http://www.semanticweb.org/andre/scuole-sicilia/ontology/RisultatiNazionali"/>
