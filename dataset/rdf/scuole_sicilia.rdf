    <sso:hasPunteggio rdf:datatype="http://www.w3.org/2001/XMLSchema#float">6</sso:hasPunteggio>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.semanticweb.org/andre/scuole-sicilia/resource/TPPM03000Q-valutazione22">
