  <rdf:Description rdf:about="http://www.semanticweb.org/andre/scuole-sicilia/resource/PAMM03600N-valutazione21">
    <rdf:type rdf:resource="http://www.semanticweb.org/andre/scuole-sicilia/ontology/RisultatiScolastici"/>
