  <rdf:Description rdf:about="http://www.semanticweb.org/andre/scuole-sicilia/resource/MEPS010008-valutazione21">
    <rdf:type rdf:resource="http://www.semanticweb.org/andre/scuole-sicilia/ontology/RisultatiScolastici"/>
