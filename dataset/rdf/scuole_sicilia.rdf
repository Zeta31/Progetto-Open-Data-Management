    <rdf:type rdf:resource="http://www.semanticweb.org/andre/scuole-sicilia/ontology/CompetenzeEuropee"/>
    <sso:hasCodiceScuola rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SRPC08000R</sso:hasCodiceScuola>
